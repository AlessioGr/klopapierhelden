// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Bestellung {
  uid        String  @id @default(cuid())
  created_at DateTime
  name       String
  amount     String
  ort        String
  email      String
  nummer     String
  done       Boolean
}